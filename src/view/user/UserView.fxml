<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.user.UserController">
   <children>
      <Button layoutX="427.0" layoutY="56.0" mnemonicParsing="false" onMouseClicked="#openSelectedAlbum" prefHeight="25.0" prefWidth="146.0" text="Open Selected Album" />
      <Button layoutX="427.0" layoutY="106.0" mnemonicParsing="false" onMouseClicked="#renameAlbum" text="Rename Selected Album" />
      <Button layoutX="427.0" layoutY="156.0" mnemonicParsing="false" onMouseClicked="#deleteAlbumDialog" prefHeight="25.0" prefWidth="146.0" text="Delete Selected Album" />
      <Button layoutX="427.0" layoutY="332.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="146.0" text="Search" />
      <Button layoutX="27.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#createAlbumDialog" prefHeight="25.0" prefWidth="146.0" text="Add Album" />
      <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#logout" text="Logout" />
      <TableView fx:id="albumsTable" layoutY="75.0" prefHeight="325.0" prefWidth="381.0" onMouseClicked="#setAlbumSelected">
        <columns>
           <TableColumn fx:id="albumNameColumn" prefWidth="125.0" text="Album" />
           <TableColumn fx:id="numberOfPhotosColumn" prefWidth="125.0" text="Number of Photos" />
           <TableColumn fx:id="dateRangeColumn" prefWidth="125.0" text="Date Range" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
