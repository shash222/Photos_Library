      <TextField fx:id="dateTakenField" editable="false" layoutX="80.0" layoutY="10.0" prefHeight="25.0" prefWidth="183.0" />
      <Label layoutX="19.0" layoutY="323.0" text="Tags:" />
      <ImageView fx:id="imageContainer" fitHeight="243.0" fitWidth="306.0" layoutX="183.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="282.0" layoutY="341.0" mnemonicParsing="false" onMouseClicked="#addTagView" prefHeight="25.0" prefWidth="164.0" text="Add New Tag" />
      <Button layoutX="282.0" layoutY="383.0" mnemonicParsing="false" text="Edit Values for Selected Tag" onMouseClicked="#editTagView" />
      <ListView fx:id="tagList" layoutX="14.0" layoutY="341.0" prefHeight="200.0" prefWidth="248.0" />
      <Button layoutX="282.0" layoutY="429.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="164.0" text="Delete Selected Tag" onMouseClicked="#deleteTag" />
   </children>
